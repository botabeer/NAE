[
  {"emoji":"👵🏻↔️👨🏻‍⚕️","text":"اسأل مجرب ولا تسأل طبيب."},
  {"emoji":"🐍➡️🐍","text":"لا يلدغ المؤمن من جحر مرتين."},
  {"emoji":"👁️➡️🤏","text":"العين بصيرة واليد قصيرة."},
  {"emoji":"⏳🗡️✂️","text":"الوقت كالسيف، إن لم تقطعه قطعك."},
  {"emoji":"🧘‍♂️🔑🔑","text":"الصبر مفتاح الفرج."},
  {"emoji":"🐦🐦➡️🐦🐦","text":"الطيور على أشكالها تقع."},
  {"emoji":"⛏️🕳️⬇️","text":"من حفر حفرة لأخيه وقع فيها."},
  {"emoji":"🛌➡️🦶","text":"على قد لحافك مد رجليك."},
  {"emoji":"💸➡️🤲➡️💸➡️🎁","text":"اصرف ما في الجيب يأتيك ما في الغيب."},
  {"emoji":"🗣️💬➡️🤫➡️💰","text":"إذا كان الكلام من فضة فالسكوت من ذهب."},
  {"emoji":"👶🎓🔨🗿","text":"العلم في الصغر كالنقش على الحجر."},
  {"emoji":"🪢🐎➡️🪢➡️🪢","text":"لسانك حصانك إن صنته صانك."},
  {"emoji":"📖🏡➡️👻","text":"البيت الذي يخلو من كتاب بيت بلا روح."},
  {"emoji":"🗣️➡️🗡️","text":"رب كلمة قالت لصاحبها دعني."},
  {"emoji":"👀🔪🐈","text":"الفضول يقتل القطط."},
  {"emoji":"🏃‍♀️➡️🐢","text":"العجلة من الشيطان، والتأني من الرحمن."},
  {"emoji":"💧⏳➡️🕳️","text":"قطرة قطرة تملأ الحجر."},
  {"emoji":"💡➡️🔦","text":"خير ما أعطي الإنسان العقل."},
  {"emoji":"🤝➡️💪","text":"يد واحدة لا تصفق."},
  {"emoji":"🔥➡️💧","text":"نار بلا دخان."},
  {"emoji":"🐒👀👩➡️🦌","text":"القرد في عين أمه غزال."},
  {"emoji":"🤝😠➡️👹","text":"احذر شر من أحسنت إليه."},
  {"emoji":"🧑‍⚖️➡️🤷‍♂️➡️🧑‍⚖️➡️😠","text":"لا تجادل بليغًا ولا سفيهًا."},
  {"emoji":"🐶➡️👋➡️🐍","text":"إذا كان لك حاجة عند الكلب قل له يا حاج."},
  {"emoji":"🪟🪨➡️🪟➡️🪟","text":"اللي بيته من إزاز ما يرميش الناس بالطوب."},
  {"emoji":"🐴🐴➡️🪞","text":"الخيل والليل والبيداء تعرفني."},
  {"emoji":"🫵➡️🫵➡️🤥","text":"من حكى لك حكى عليك."},
  {"emoji":"👥️👥️➡️🤷‍♀️","text":"كثرة الأيادي تفسد الطبخة."},
  {"emoji":"👤➡️👤➡️🚫👤","text":"من عاش بوجهين مات لا وجه له."},
  {"emoji":"🎭➡️🎭➡️😭","text":"اضحك للدنيا تضحكلك."},
  {"emoji":"😈➡️😴","text":"نام وارتاح يأتيك النجاح."},
  {"emoji":"💰💸❌➡️😡","text":"الفقر ليس عيباً."},
  {"emoji":"🕊️🕊️➡️🕊️➡️🕊️","text":"الطيور على أشكالها تقع."},
  {"emoji":"🤐🤫➡️💰","text":"الصمت حكمة."},
  {"emoji":"🚶‍♂️➡️🌳➡️🚶‍♂️","text":"امشِ في جنازة ولا تمشِ في جوازة."},
  {"emoji":"👀➡️👨‍👩‍👦","text":"الولد سر أبيه."},
  {"emoji":"🚪💨➡️🤫😴","text":"الباب اللي يجي منه الريح، سده واستريح."},
  {"emoji":"🤲➡️💧➡️🔥","text":"اللي إيده في الميه مش زي اللي إيده في النار."},
  {"emoji":"🤷‍♀️🤦‍♀️➡️🤷‍♂️🤷‍♂️","text":"عذر أقبح من ذنب."},
  {"emoji":"🧠➡️💎","text":"العقل زينة."},
  {"emoji":"🦅🔪🔥","text":"اللي ما يعرف الصقر يشويه."},
  {"emoji":"👩‍🦱➡️🐴➡️🏃‍♀️","text":"مع الخيل يا شقرا."},
  {"emoji":"🍇❌➡️😠🗣️","text":"اللي ما يطول العنب حامض عنه يقول."},
  {"emoji":"🏠➡️🪵➡️🌪️","text":"إذا كان ساس البيت مبني على الطيب، فكثرة عواصف الزمن ما تهده."},
  {"emoji":"🐴➡️🧓➡️🐴➡️👴","text":"العاقل من اتعظ بغيره."},
  {"emoji":"💍➡️🫵","text":"الخاتم يضيق على الإصبع."},
  {"emoji":"👤🤷‍♂️➡️🗣️","text":"لو كان الكلام دواء، لكان الصمت شفاء."},
  {"emoji":"🏠➡️🌾➡️🪆","text":"من فات قديمه تاه."},
  {"emoji":"🥚➡️🥚➡️🥚","text":"ما كل بيضة شحمة."},
  {"emoji":"👃➡️🤷‍♀️","text":"نفسه في راس خشمه."},
  {"emoji":"🐑➡️🐺➡️🐑➡️🐺","text":"ما كل من يلبس قناع الذئب ذئب."},
  {"emoji":"🔥➡️🪵","text":"كالمستجير من الرمضاء بالنار."},
  {"emoji":"🤫➡️🤷‍♂️","text":"من سكت عن الحق شيطان أخرس."},
  {"emoji":"🐪🐪🔪🔪","text":"إذا وقع الجمل كثرت سكاكينه."},
  {"emoji":"🐟➡️🌊","text":"عصفور باليد خير من عشرة على الشجرة."},
  {"emoji":"💰❌➡️😭➡️💰","text":"السلف تلف والرد خسارة."},
  {"emoji":"👵➡️👧","text":"اقلب القدرة على فمها تطلع البنت لأمها."},
  {"emoji":"📰➡️💸➡️📰➡️🆓","text":"يا خبر النهاردة بفلوس بكرة يبقى ببلاش."},
  {"emoji":"🤷‍♀️🤷‍♀️➡️🤷‍♀️🤷‍♀️➡️🤷‍♀️","text":"كثر السلام يقل المعرفة."},
  {"emoji":"😔➡️🥳➡️🚫","text":"جت الحزينة تفرح، ما لقتلهاش مطرح."},
  {"emoji":"🚶‍♂️➡️➡️😈","text":"امشي عدل يحتار عدوك فيك."},
  {"emoji":"🧑‍🤝‍🧑➡️➡️🧑‍🤝‍🧑","text":"ما محبة إلا بعد عداوة."},
  {"emoji":"💸➡️🐵➡️💸➡️🐵","text":"يا واخد القرد على ماله، يروح المال ويبقى القرد على حاله."},
  {"emoji":"👨‍👩‍👧➡️🤷‍♀️🤷‍♂️","text":"ابن الحلال عند ذكر الشر يطل."},
  {"emoji":"🎣➡️💧➡️🔥","text":"اللي إيده في الميه مش زي اللي إيده في النار."},
  {"emoji":"🤝➡️🤬","text":"احترس من عدوك مرة ومن صديقك ألف مرة."},
  {"emoji":"🧑‍🤝‍🧑➡️🤷‍♀️","text":"اكل العيش يحب الخبز."},
  {"emoji":"🏠➡️🏚️","text":"على قدّ لحافك مدّ رجليك."},
  {"emoji":"🤫➡️👤","text":"السكوت علامة الرضا."},
  {"emoji":"💰➡️⚖️","text":"المال السايب يعلم السرقة."},
  {"emoji":"🏡➡️🌊➡️💨","text":"طب الجرة على تمها بتطلع البنت لإمها."},
  {"emoji":"🚪➡️🚪","text":"الباب اللي بيجيك منه الريح، سده واستريح."},
  {"emoji":"🕰️➡️⏳","text":"الوقت يداوي كل الجروح."},
  {"emoji":"🐾➡️🐾","text":"من رافق الكذاب كذب عليه."},
  {"emoji":"💧🌊➡️🤿➡️🤥","text":"المي بتكذب الغطاس."},
  {"emoji":"🐶➡️🐕","text":"ديل الكلب عمره ما يتعدل."},
  {"emoji":"🗣️➡️👁️","text":"الحكي مش متل الشوفة."},
  {"emoji":"🕊️➡️🌲","text":"عصفور باليد ولا عشرة على الشجرة."},
  {"emoji":"🆕➡️🆓","text":"اللي ما عنده عتيق، ما عنده جديد."},
  {"emoji":"🧑‍🤝‍🧑➡️➡️🤫","text":"الصداقة كنز لا يفنى."},
  {"emoji":"👀➡️👂","text":"ليس كل ما يلمع ذهباً."},
  {"emoji":"🪝➡️🐠","text":"السمكة تبدأ بالتعفن من الرأس."},
  {"emoji":"🍎➡️👨‍👩‍👧","text":"الابن سر أبيه."},
  {"emoji":"🧠➡️💪","text":"العقل السليم في الجسم السليم."},
  {"emoji":"🔑🔑➡️🚪","text":"لكل مشكلة حل."},
  {"emoji":"🤝➡️🧑‍🤝‍🧑","text":"التعاون يجلب النجاح."},
  {"emoji":"💧➡️🪨","text":"قطرة ماء تهدم صخرة."},
  {"emoji":"🪢➡️🎁","text":"الحبل على الجرار."},
  {"emoji":"🎁➡️🎁","text":"اطلب العلم من المهد إلى اللحد."},
  {"emoji":"🗣️➡️🤫➡️💰","text":"الصمت حكمة."},
  {"emoji":"🐦➡️🦅","text":"كل إناء بما فيه ينضح."},
  {"emoji":"⚖️👑➡️👑","text":"العدل أساس الملك."},
  {"emoji":"🌳➡️🌳","text":"الغني غني النفس."},
  {"emoji":"➡️🏃‍♂️💨","text":"الساكت عن الحق شيطان أخرس."},
  {"emoji":"➡️🚫➡️🙅‍♀️","text":"لا يضيع حق وراءه مطالب."},
  {"emoji":"😈➡️😈➡️👼","text":"لكل شيطان مريد."},
  {"emoji":"🤫➡️🤷‍♀️","text":"السكوت من ذهب."},
  {"emoji":"🤝➡️➡️🗣️","text":"يد الله مع الجماعة."},
  {"emoji":"💰➡️🚫➡️💸","text":"المال الحرام ما بيدوم."},
  {"emoji":"👑➡️👑","text":"القوة في الوحدة."}
]
